{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang++ build active file",
            "command": "/usr/bin/clang++",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-std=c++17",
                "-O3",
                "${workspaceFolder}/MR_MT_Solver/src/problem_generator.cpp",
                "-I/opt/homebrew/Cellar/tbb/2021.9.0/include/",
                "-I/opt/homebrew/Cellar/tbb/2021.9.0/include/oneapi/",
                "${workspaceFolder}/MR_MT_Solver/src/GA.cpp",
                "${workspaceFolder}/MR_MT_Solver/src/logger.cpp",
                "${workspaceFolder}/MR_MT_Solver/src/model.cpp",
                "${workspaceFolder}/MR_MT_Solver/MR_MT_Solver.cpp",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-L/opt/homebrew/Cellar/tbb/2021.9.0/lib/",
                "-ltbb"
                // "-arch",
                // "arm64",  // Specify the architecture
            ],            
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-std=c++17"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ],
